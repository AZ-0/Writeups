#!/usr/bin/env sage
from Crypto.Util.number import getPrime

with open('flag.txt','rb') as f:
    FLAG = f.read()
    f.close()

n = getPrime(512) * getPrime(512)
e = 0x10001

P.<x> = GF(2)[]
N = sum([int(b) * x^i for i, b in enumerate(bin(n)[2:])])
Q.<x> = P.quotient(N)

def encrypt(msg):
    if type(msg) == bytes:
        msg = int.from_bytes(msg, 'big')

    msg = sum([int(b) * x^i for i, b in enumerate('{:0{bl}b}'.format(msg, bl=n.bit_length()-1))])
    cip = Q(msg)^e
    cip = int(''.join([str(i) for i in list(cip)]), 2)
    return cip

c = encrypt(FLAG)

print('n =', n)
print('e =', e)
print('c =', c)

